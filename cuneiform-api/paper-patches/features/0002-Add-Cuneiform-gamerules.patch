diff --git a/src/main/java/org/bukkit/GameRules.java b/src/main/java/org/bukkit/GameRules.java
index c1ad190d537b6b7086ac78c6f5cdecc0accd30dc..a58b688024427dfed9d51dd25368e35df500d909 100644
--- a/src/main/java/org/bukkit/GameRules.java
+++ b/src/main/java/org/bukkit/GameRules.java
@@ -136,6 +136,12 @@ public final class GameRules {
         return (GameRule<T>) Registry.GAME_RULE.getOrThrow(NamespacedKey.minecraft(key));
     }
 
+    // Cuneiform Start - Cuneiform Gamerules
+    private static <T> GameRule<T> getCuniformRule(@KeyPattern.Value String key) {
+        return (GameRule<T>) Registry.GAME_RULE.getOrThrow(NamespacedKey.cuneiform(key));
+    }
+    // Cuneiform End
+
     private GameRules() {
     }
 }
diff --git a/src/main/java/org/bukkit/NamespacedKey.java b/src/main/java/org/bukkit/NamespacedKey.java
